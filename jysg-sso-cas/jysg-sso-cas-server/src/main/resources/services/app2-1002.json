  "name" : "测试客户端app1",
  "id" : 1002,
  "description" : "这是app2的客户端",
  "evaluationOrder" : 12,
  "theme" : "app2",
  "attributeReleasePolicy" : {
    "@class" : "org.apereo.cas.services.ReturnAllowedAttributeReleasePolicy",
    "allowedAttributes" : [ "java.util.ArrayList", [ "name", "phone" ] ]
  }
